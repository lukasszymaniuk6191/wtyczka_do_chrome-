{
  "manifest_version": 2,
  "name": "My Vocabulary Extension",
  "version": "0.1",

  "browser_action": {
  "default_icon": "icon.png",
  "default_popup": "popup.html"
  },

  "background": {
  "scripts": ["background.js"]
  },

  "permissions": [
      "activeTab",
      "http://localhost:8080/my-vocabulary/"
   ]
    



}
